//baekjoon 1920. 수 찾기

#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
//퀵정렬 함수
int compare(const int* a, const int* b) {
	int A = *(int*)a;
	int B = *(int*)b;

	if (A > B) {
		return 1;
	}
	else return -1;
}
//이진탐색
int binary(int arr[], int k, int begin, int end) {
	int mid;
	if (begin == end) {
		if (arr[begin] == k) {
			return 1;
		}
		else {
			return 0;
		}
	}
	mid = (begin + end) / 2;
	if (arr[mid] == k) {
		return 1;
	}
	else if (arr[mid] > k && (mid - 1 >= begin)) {
		binary(arr, k, begin, mid - 1);
	}
	else if (arr[mid] < k && (mid + 1 <= end)) {
		binary(arr, k, mid+1, end);
	}
	else {
		return 0;
	}
	
	return ;
}

int main() {
	int n, m;
	int num[100001] = { 0 };
	int cmp[100001] = { 0 };
	int answer;

	scanf("%d", &n);
	for (int i = 0;i < n;i++) {
		scanf("%d", &num[i]);
	}
	qsort(num, n, sizeof(int), compare);

	scanf("%d", &m);
	for (int i = 0;i < m;i++) {
		scanf("%d", &cmp[i]);
	}

	for (int i = 0;i < m;i++) {
		answer = binary(num, cmp[i], 0, n - 1);
		if (answer == 1) {
			printf("%d\n", answer);
		}
		else {
			answer = 0;
			printf("%d\n", answer);
		}
	}
}
