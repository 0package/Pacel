//baekjoon 12789.도키도키 간식드리미 (스택문제)

#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	int n;
	int list[1001] = { 0 }, l = 0;
	int stk[1001] = { 0 }, s = -1;
	int answer[1001] = { 0 }, a = 0;
	int i = 1, check = 0;

	scanf("%d", &n);
	for (int j = 0; j < n; j++) {
		scanf("%d", &list[j]);
	}

	while (1) {
		if (list[l] == i) {
			answer[a] = i;
			list[l] = 0;
			a++;
			i++;
			l++;
		}
		else if (list[l] != i) {
			if (s < 0) {
				s++;
				stk[s] = list[l];
				list[l] = 0;
				l++;
			}
			else if (s >= 0) {
				if (stk[s] == i) {
					answer[a] = i;
					s--;
					a++;
					i++;
				}
				else if (stk[s] != i) {
					s++;
					stk[s] = list[l];
					list[l] = 0;
					l++;
				}
			}
		}
		if (l == n) {
			break;
		}
	}

	if (s >= 0) {
		for (int j = s; j >= 0; j--) {
			if (stk[j] == i) {
				i++;
			}
			else if (stk[j] != i) {
				check = 1;
				printf("Sad");
				break;
			}
		}
	}


	if (check == 0) {
		printf("Nice");
	}
	
}
